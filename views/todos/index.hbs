<header class="d-flex justify-content-between align-items-center">
  <h1>Todos</h1>
  <a href="/todos/new" class="btn btn-primary">New To Do</a>
</header>
<ul class='list-group'>
    {{#each todoURL}}
        <li class='list-group-item d-flex justify-content-between'>
            {{#if this.complete}}
            <div class="text-muted"><s>{{this.index}}-{{this.title}}</s></div>
            {{else}}
            <div>{{this.index}}-{{this.title}}</div>
            {{/if}}
            <div class='links'>
                <a href='/todos/{{this._id}}' class="btn btn-info">View</a>
                <a href="/todos/{{this._id}}/edit" class="btn btn-primary">Edit</a>
                <a href="https://google.com/?q={{this.titleEncoded}}" class="btn btn-info" >Google It</a>
                <form action="/todos/{{this._id}}?_method=DELETE" method="POST" class="d-inline">
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </form>   
            </div>
        </li>
    {{/each}}
</ul>